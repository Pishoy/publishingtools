http://advisors.threefold.me, https://advisors.threefold.me {
    redir {
           if {scheme} is http
           / https://{host}{uri}
    } 
    basicauth / user password 

    tls self_signed
    proxy / localhost:3000/advisors
}

http://board.threefold.me, https://board.threefold.me {
    redir {
        if {scheme} is http
        / https://{host}{uri}
    } 
    basicauth / user password 

    tls self_signed
    proxy / localhost:3000/board
}

