http://advisors.threefold.me {
    redir {
           if {scheme} is http
           / https://{host}{uri}
    }
}


https://advisors.threefold.me {

    tls info@threefold.io
    proxy /auth localhost:3000 {
        transparent
    }
    redir /advisors/ /


    proxy / localhost:3000/advisors
}


